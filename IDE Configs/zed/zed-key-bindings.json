{
  "agent": {
    "dock": "left",
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4.1"
    },
    "play_sound_when_agent_done": true
  },
  "edit_predictions": {
    "mode": "eager",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    }
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "terminal": {
    "dock": "bottom",
    "font_size": 18,
    "line_height": "standard"
  },
  // Related to font
  "ui_font_size": 16, // Edited
  "buffer_font_size": 20, // Edited
  "buffer_font_family": "Fira Code",
  "buffer_line_height": {
    // Custom Line Height
    "custom": 2.2
  },
  "buffer_font_weight": 200,
  // Theme
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "Catppuccin Mocha"
  },
  // Additionals
  "unnecessary_code_fade": 0.2,
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  }
}
